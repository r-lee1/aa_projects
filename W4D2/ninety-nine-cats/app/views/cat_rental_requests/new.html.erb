<form action="<%= cat_rental_requests_url %>" method="post">
  <select name="cat_rental_request[cat_id]">
      <% Cat.all.each do |cat|%>
      <option value=" <%= cat.id %>"> <%= cat.name %></option>
      <% end %>
  </select>
  <label for="Start Date">Start Date: </label>
  <input id="Start Date" type="date" name="cat_rental_request[start_date]" value="">
  <label for="End Date">End Date: </label>
  <input id="End Date" type="date" name="cat_rental_request[end_date]" value="">
  <input type="submit" value="Create Request">
</form>
